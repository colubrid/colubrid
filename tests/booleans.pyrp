    ['print', ('true', ['op'], 'true', ['op'], 'true',
               [['op'], (true, true, true)])],
    ['print', ('true', ['op'], 'true', ['op'], 'false',
               [['op'], (true, true, false)])],
    ['print', ('false', ['op'], 'true', ['op'], 'true',
               [['op'], (false, true, true)])],
    ['print', ('false', ['op'], 'true', ['op'], 'false',
               [['op'], (false, true, false)])],
    ['print', ('false', ['op'], 'false', ['op'], 'true',
               [['op'], (false, false, true)])],
    ['print', ('false', ['op'], 'false', ['op'], 'false',
               [['op'], (false, false, false)])]
)]},

{'op': 'and'}, ['testbin', ()],
{'op': 'or'}, ['testbin', ()],

['print', ('= Testing not operator =')],
['print', ('not true', ['not', (true)])],
['print', ('not false', ['not', (false)])],
['print', ('not true and not true', ['not', (true, true)])],
['print', ('not true and not false', ['not', (true, false)])],
['print', ('not false and not true', ['not', (false, true)])],
['print', ('not false and not false', ['not', (false, false)])]
]
